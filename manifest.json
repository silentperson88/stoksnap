{
  "manifest_version": 3,
  "name": "StockSnap",
  "version": "1.0",
  "description": "Test extension to run Python code with a Chrome plugin",
  "permissions": ["scripting", "activeTab", "tabs"],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": "icon.png"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ]
}
